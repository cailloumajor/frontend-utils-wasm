{
  "name": "@cailloumajor/frontend-utils-wasm",
  "version": "6.0.1",
  "exports": "./pkg/frontend_utils_wasm.js",
  "exclude": [
    "target/"
  ],
  "publish": {
    "include": [
      "pkg/",
      "README.md"
    ]
  },
  "unstable": [
    "bundle"
  ],
  "tasks": {
    "wasmbuild": "deno run -A jsr:@deno/wasmbuild@0.19.3",
    "build": "deno task wasmbuild --inline --out pkg",
    "test": "deno test -A",
    "update_snapshot": "deno test -A -- --update-snapshot"
  },
  "imports": {
    "@astral/astral": "jsr:@astral/astral@0.5.4",
    "@std/assert": "jsr:@std/assert@1.0.14",
    "@std/http": "jsr:@std/http@1.0.20",
    "@std/path": "jsr:@std/path@1.1.2",
    "imagescript": "jsr:@matmen/imagescript@1.3.1",
    "pixelmatch": "npm:pixelmatch@7.1.0"
  },
  "fmt": {
    "lineWidth": 100,
    "semiColons": false
  },
  "compilerOptions": {
    "lib": [
      "deno.ns",
      "deno.unstable",
      "dom"
    ]
  }
}
